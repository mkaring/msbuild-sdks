<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<ImportCoreExtensionTargets Condition="'$(ImportCoreExtensionTargets)' == ''">true</ImportCoreExtensionTargets>
		<ImportLegacyExtensionTargets Condition="'$(ImportLegacyExtensionTargets)' == ''">true</ImportLegacyExtensionTargets>
		<ImportProjectExtensionTargets Condition="'$(ImportProjectExtensionTargets)' == ''">true</ImportProjectExtensionTargets>
		<ImportDirectoryBuildTargets Condition="'$(ImportDirectoryBuildTargets)' == ''">true</ImportDirectoryBuildTargets>

		<MSBuildCoreExtensionTargetsPath Condition="'$(MSBuildCoreExtensionTargetsPath)' == '' OR !Exists('$(MSBuildCoreExtensionTargetsPath)')">$(MSBuildThisFileDirectory)MSBuild.ProjectDefaults.targets</MSBuildCoreExtensionTargetsPath>
	</PropertyGroup>

	<ImportGroup>
		<Import Project="$(MSBuildCoreExtensionTargetsPath)" Condition="'$(ImportCoreExtensionTargets)' == 'true'" />
		<Import Project="$(MSBuildThisFileDirectory)..\Extensions\MSBuild.ProjectExtensions.targets" Condition="'$(ImportProjectExtensionTargets)' == 'true'" />
		<Import Project="$(MSBuildThisFileDirectory)Directory.Build.targets" Condition="'$(ImportDirectoryBuildTargets)' == 'true'" />
	</ImportGroup>

	<PropertyGroup>
		<ImportedMSBuildCoreSdkTargets>true</ImportedMSBuildCoreSdkTargets>
	</PropertyGroup>

</Project>