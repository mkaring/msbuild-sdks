<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<ImportLegacyExtensionTargets Condition="'$(ImportLegacyExtensionTargets)' == ''">true</ImportLegacyExtensionTargets>
		<ImportProjectDefaultTargets Condition="'$(ImportProjectDefaultTargets)' == ''">true</ImportProjectDefaultTargets>
		<ImportProjectExtensionTargets Condition="'$(ImportProjectExtensionTargets)' == ''">true</ImportProjectExtensionTargets>
		<ImportDirectoryBuildTargets Condition="'$(ImportDirectoryBuildTargets)' == ''">true</ImportDirectoryBuildTargets>
	</PropertyGroup>

	<ImportGroup>
		<Import Project="$(MSBuildThisFileDirectory)..\Extensions\MSBuild.LegacyExtensions.targets" Condition="'$(ImportLegacyExtensionTargets)' == 'true'" />
		<Import Project="$(MSBuildThisFileDirectory)MSBuild.ProjectDefaults.props" Condition="'$(ImportProjectDefaultTargets)' == 'true' and '$(ImportLegacyExtensionTargets)' != 'true'" />
		<Import Project="$(MSBuildThisFileDirectory)..\Extensions\MSBuild.ProjectExtensions.targets" Condition="'$(ImportProjectExtensionTargets)' == 'true'" />
		<Import Project="$(MSBuildThisFileDirectory)Directory.Build.targets" Condition="'$(ImportDirectoryBuildTargets)' == 'true'" />
	</ImportGroup>

	<PropertyGroup>
		<ImportedMSBuildCoreSdkTargets>true</ImportedMSBuildCoreSdkTargets>
	</PropertyGroup>

</Project>