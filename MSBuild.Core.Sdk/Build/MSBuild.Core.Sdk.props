<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<ImportDirectoryBuildProps Condition="'$(ImportDirectoryBuildProps)' == ''">true</ImportDirectoryBuildProps>
		<ImportProjectExtensionProps Condition="'$(ImportProjectExtensionProps)' == ''">true</ImportProjectExtensionProps>
		<ImportCoreExtensionProps Condition="'$(ImportCoreExtensionProps)' == ''">true</ImportCoreExtensionProps>
	</PropertyGroup>

	<PropertyGroup Condition="'$(MSBuildCoreExtensionPropsPath)' == ''">
		<ImportLegacyExtensionProps Condition="'$(ImportLegacyExtensionProps)' == ''">true</ImportLegacyExtensionProps>
		<MSBuildCoreExtensionPropsPath Condition="'$(ImportLegacyExtensionProps)' != 'true'">$(MSBuildThisFileDirectory)MSBuild.ProjectDefaults.props</MSBuildCoreExtensionPropsPath>
		<MSBuildCoreExtensionPropsPath Condition="'$(ImportLegacyExtensionProps)' == 'true'">$(MSBuildThisFileDirectory)..\Extensions\MSBuild.LegacyExtensions.props</MSBuildCoreExtensionPropsPath>
	</PropertyGroup>

	<ImportGroup>
		<Import Project="$(MSBuildThisFileDirectory)Directory.Build.props" Condition="'$(ImportDirectoryBuildProps)' == 'true'" />
		<Import Project="$(MSBuildThisFileDirectory)..\Extensions\MSBuild.ProjectExtensions.props" Condition="'$(ImportProjectExtensionProps)' == 'true'" />
		<Import Project="$(MSBuildCoreExtensionPropsPath)" Condition="'$(ImportCoreExtensionProps)' == 'true'" />
	</ImportGroup>

	<PropertyGroup>
		<ImportedMSBuildCoreSdkProps>true</ImportedMSBuildCoreSdkProps>
	</PropertyGroup>

</Project>