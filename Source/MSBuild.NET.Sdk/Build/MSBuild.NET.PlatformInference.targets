<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<_SdkPlatformName Condition="'$(TargetPlatformIdentifier)' == ''">Windows</_SdkPlatformName>
		<_SdkPlatformName Condition="'$(TargetPlatformIdentifier)' == ''">Linux</_SdkPlatformName>
		<_SdkPlatformName Condition="'$(TargetPlatformIdentifier)' == ''">Mac</_SdkPlatformName>
		<_SdkPlatformName Condition="'$(_SdkPlatformName)' != ''">$(TargetPlatformIdentifier)</_SdkPlatformName>
	</PropertyGroup>

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>