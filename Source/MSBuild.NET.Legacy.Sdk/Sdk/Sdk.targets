<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup Condition="'$(MicrosoftLegacyTargetsFileName)' != ''">
		<CustomProjectCoreTargetsPath>$(MSBuildThisFileDirectory)..\$(MicrosoftLegacyTargetsFileName)</CustomProjectCoreTargetsPath>
	</PropertyGroup>

	<PropertyGroup Condition="'$(CustomProjectCoreTargetsPath)' == ''">
		<CustomProjectCoreTargetsPath Condition="'$(MSBuildProjectExtension)' == '.csproj'">$(MSBuildThisFileDirectory)..\Microsoft.CSharp.targets</CustomProjectCoreTargetsPath>
		<CustomProjectCoreTargetsPath Condition="'$(MSBuildProjectExtension)' == '.vbproj'">$(MSBuildThisFileDirectory)..\Microsoft.VisualBasic.targets</CustomProjectCoreTargetsPath>
		<CustomProjectCoreTargetsPath>$(MSBuildThisFileDirectory)..\Microsoft.Common.targets</CustomProjectCoreTargetsPath>
	</PropertyGroup>

	<Import Project="$(MSBuildThisFileDirectory)..\Microsoft.Legacy.props" Condition="'$(ImportedMicrosoftLegacyProps)' != 'true'"/>
	<Import Project="Sdk.targets" Sdk="MSBuild.Core.Sdk" Condition="'$(ImportedMSBuildCoreSdkTargets)' != 'true'"/>

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>