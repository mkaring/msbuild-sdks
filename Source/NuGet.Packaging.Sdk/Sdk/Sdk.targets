		<CustomProjectExtensionTargets>$(MSBuildThisFileDirectory)..\Build\NuGet.Packaging.Sdk.targets</CustomProjectExtensionTargets>
	</PropertyGroup>

	<Import Project="Sdk.targets" Sdk="MSBuild.Core.Sdk"/>
	<Import Project="Sdk.Restore.targets"/>

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>