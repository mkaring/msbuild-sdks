<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<ImportLegacyTargets Condition="'$(ImportLegacyTargets)' == ''">true</ImportLegacyTargets>
		<CustomCoreTargetsPath Condition="'$(CustomCoreTargetsPath)' == '' OR !Exists('$(CustomCoreTargetsPath)')">$(MSBuildThisFileDirectory)..\Core\MSBuild.Core.targets</CustomCoreTargetsPath>
	</PropertyGroup>

	<Import Project="$(CustomCoreTargetsPath)" Condition="'$(ImportLegacyTargets)' != 'true'" />

	<ImportGroup Condition="'$(ImportLegacyTargets)' == 'true'">
		<Import Project="$(MSBuildThisFileDirectory)..\Legacy\Microsoft.Legacy.props" />
		<Import Project="$(MSBuildLegacySdkPath)\Microsoft.Common.targets" />
	</ImportGroup>

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>