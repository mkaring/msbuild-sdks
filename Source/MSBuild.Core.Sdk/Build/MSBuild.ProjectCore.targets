<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<ImportLegacyTargets Condition="'$(ImportLegacyTargets)' == ''">true</ImportLegacyTargets>
		<CustomProjectCoreTargetsPath Condition="'$(CustomProjectCoreTargetsPath)' == '' OR !Exists('$(CustomProjectCoreTargetsPath)')">$(MSBuildThisFileDirectory)..\Core\MSBuild.Core.targets</CustomProjectCoreTargetsPath>
	</PropertyGroup>

	<Import Project="$(CustomProjectCoreTargetsPath)" Condition="'$(ImportLegacyTargets)' != 'true'" />

	<ImportGroup Condition="'$(ImportLegacyTargets)' == 'true'">
		<Import Project="$(MSBuildThisFileDirectory)..\Legacy\Microsoft.Legacy.props" />
		<Import Project="$(MSBuildLegacySdkPath)\Microsoft.Common.targets" />
	</ImportGroup>

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>