<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<CustomProjectCorePropsPath Condition="'$(CustomProjectCorePropsPath)' == '' OR !Exists('$(CustomProjectCorePropsPath)')">$(MSBuildThisFileDirectory)..\Core\MSBuild.Core.props</CustomProjectCorePropsPath>
	</PropertyGroup>

	<Import Project="$(CustomProjectCorePropsPath)" />

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>