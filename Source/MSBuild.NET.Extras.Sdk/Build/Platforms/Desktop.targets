<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup Condition="'$(FrameworkProjectType)' == 'Wpf'">
		<EnableWpfProjectSetup>true</EnableWpfProjectSetup>
		<EnableAspNetProjectSetup>false</EnableAspNetProjectSetup>
		<EnableWinFormsProjectSetup>false</EnableWinFormsProjectSetup>
	</PropertyGroup>

	<PropertyGroup Condition="'$(FrameworkProjectType)' == 'AspNet'">
		<EnableWpfProjectSetup>false</EnableWpfProjectSetup>
		<EnableAspNetProjectSetup>true</EnableAspNetProjectSetup>
		<EnableWinFormsProjectSetup>false</EnableWinFormsProjectSetup>
	</PropertyGroup>

	<PropertyGroup Condition="'$(FrameworkProjectType)' == 'WinForms'">
		<EnableWpfProjectSetup>false</EnableWpfProjectSetup>
		<EnableAspNetProjectSetup>false</EnableAspNetProjectSetup>
		<EnableWinFormsProjectSetup>true</EnableWinFormsProjectSetup>
	</PropertyGroup>

	<PropertyGroup Condition="'$(UsingLegacyProjectSystem)' == 'true'">
		<ProjectTypeGuids Condition="'$(EnableWpfProjectSetup)' == 'true'">$(ProjectTypeGuids);{60DC8134-EBA5-43B8-BCC9-BB4BC16C2548}</ProjectTypeGuids>
		<ProjectTypeGuids Condition="'$(EnableAspNetProjectSetup)' == 'true'">$(ProjectTypeGuids);{349C5851-65DF-11DA-9384-00065B846F21}</ProjectTypeGuids>
		<ProjectTypeGuids Condition="'$(EnableWinFormsProjectSetup)' == 'true'">$(ProjectTypeGuids);{}</ProjectTypeGuids>
	</PropertyGroup>

	<PropertyGroup Condition="$(_ShortFrameworkProfileCount) == 1 AND $(_ShortFrameworkProfile.Contains('+client+'))">
		<TargetFrameworkProfile>Client</TargetFrameworkProfile>
		<IncludeProjectPriFile>false</IncludeProjectPriFile>
		<!-- .NET Client profiles defines as NET35_CLIENT, NET40_CLIENT etc -->
		<ImplicitFrameworkDefine Condition="'$(DisableImplicitFrameworkDefines)' != 'true'">$(_ShortFrameworkIdentifierWithVersion.ToUpperInvariant())_CLIENT</ImplicitFrameworkDefine>
		<DisableImplicitFrameworkDefines Condition="'$(DisableImplicitFrameworkDefines)' != 'true'">true</DisableImplicitFrameworkDefines>
	</PropertyGroup>

	<PropertyGroup>
		<EnableDefaultSettingsItems Condition="'$(EnableDefaultSettingsItems)' == ''">true</EnableDefaultSettingsItems>
		<EnableSettingsCodeGenerator Condition="'$(EnableSettingsCodeGenerator)' == ''">true</EnableSettingsCodeGenerator>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableSettingsCodeGenerator)' == 'true'">
		<SettingsCodeGeneratorTool Condition="'$(SettingsGeneratedCodeIsPublic)' != 'true'">SettingsSingleFileGenerator</SettingsCodeGeneratorTool>
		<SettingsCodeGeneratorTool Condition="'$(SettingsGeneratedCodeIsPublic)' == 'true'">PublicSettingsSingleFileGenerator</SettingsCodeGeneratorTool>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableWpfProjectSetup)' == 'true'">
		<EnableDefaultWpfItems Condition="'$(EnableDefaultWpfItems)' == ''">true</EnableDefaultWpfItems>
		<EnableImplicitWpfReferences Condition="'$(EnableImplicitWpfReferences)' == ''">true</EnableImplicitWpfReferences>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableAspNetProjectSetup)' == 'true'">
		<EnableDefaultAspNetItems Condition="'$(EnableDefaultAspNetItems)' == ''">true</EnableDefaultAspNetItems>
		<EnableImplicitAspNetReferences Condition="'$(EnableImplicitAspNetReferences)' == ''">true</EnableImplicitAspNetReferences>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableWinFormsProjectSetup)' == 'true'">
		<EnableDefaultWinFormsItems Condition="'$(EnableDefaultWinFormsItems)' == ''">true</EnableDefaultWinFormsItems>
		<EnableImplicitWinFormsReferences Condition="'$(EnableImplicitWinFormsReferences)' == ''">true</EnableImplicitWinFormsReferences>
		<EnableUserControlDesigner Condition="'$(EnableUserControlDesigner)' == ''">false</EnableUserControlDesigner>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableDefaultWpfItems)' == 'true' AND '$(ApplicationDefinitionFile)' != ''">
		<ApplicationDefinitionFile Condition="$(ApplicationDefinitionFile.Contains(';'))"/>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableDefaultWpfItems)' == 'true' AND '$(ApplicationDefinitionFile)' == ''">
		<ApplicationDefinitionFile Condition="Exists('$(MSBuildProjectDirectory)\App.xaml')">$(MSBuildProjectDirectory)\App.xaml</ApplicationDefinitionFile>
		<ApplicationDefinitionFile Condition="Exists('$(MSBuildProjectDirectory)\Application.xaml')">$(MSBuildProjectDirectory)\Application.xaml</ApplicationDefinitionFile>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableDefaultWpfItems)' == 'true'">
		<EnableDefaultPageItems Condition="'$(EnableDefaultPageItems)' == ''">true</EnableDefaultPageItems>
		<EnableDefaultResourceItems Condition="'$(EnableDefaultResourceItems)' == ''">true</EnableDefaultResourceItems>

		<DefaultResourceIncludes Condition="'$(DefaultResourceIncludes)' == ''">Themes\*.xaml;Resources\*.xaml</DefaultResourceIncludes>
		<DefaultPageExcludes Condition="'$(DefaultPageExcludes)' != ''">$(DefaultPageExcludes);</DefaultPageExcludes>
		<DefaultPageExcludes>$(DefaultPageExcludes)$(ApplicationDefinitionFile);$(DefaultResourceIncludes)</DefaultPageExcludes>
	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableDefaultAspNetItems)' == 'true'">

	</PropertyGroup>

	<PropertyGroup Condition="'$(EnableDefaultWinFormsItems)' == 'true'">
		<UserControlDesignerService Condition="'$(UserControlDesignerService)' == ''">{94E38DFF-614B-4CBD-B67C-F211BB35CE8B}</UserControlDesignerService>
	</PropertyGroup>

</Project>