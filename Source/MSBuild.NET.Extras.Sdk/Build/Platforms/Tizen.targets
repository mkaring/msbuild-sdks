<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<!-- Default to v4 since it's the lowest one that supports .NET Standard and is public -->
		<TargetFrameworkVersion Condition="'$(TargetFrameworkVersion)' == ''">v4.0</TargetFrameworkVersion>
		<DisableStandardFrameworkResolution>true</DisableStandardFrameworkResolution>
		<AutoUnifyAssemblyReferences>true</AutoUnifyAssemblyReferences>
	</PropertyGroup>

	<!-- This gets TIZEN and TIZEN40 -->
	<PropertyGroup Condition="'$(DisableImplicitFrameworkDefines)' != 'true'">
		<ImplicitFrameworkDefine>$(TargetFrameworkIdentifier.ToUpperInvariant());$(TargetFramework.ToUpperInvariant())</ImplicitFrameworkDefine>
		<DisableImplicitFrameworkDefines>true</DisableImplicitFrameworkDefines>
	</PropertyGroup>

</Project>
