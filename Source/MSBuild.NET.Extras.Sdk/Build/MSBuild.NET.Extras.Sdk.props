<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<Choose>
		<When Condition="'$(MSBuildProjectExtension)' == '.csproj' OR ('$(MSBuildProjectExtension)' == '.tmp_proj' AND Exists('$(MSBuildProjectDirectory)\$(AssemblyName).csproj'))">
			<PropertyGroup>
				<_SdkLanguageName>CSharp</_SdkLanguageName>
				<_SdkLanguageExtension>.cs</_SdkLanguageExtension>
				<_SdkLanguageGuid>FAE04EC0-301F-11D3-BF4B-00C04F79EFBC</_SdkLanguageGuid>
				<_SdkDesignerFolder>Properties</_SdkDesignerFolder>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.vbproj' OR ('$(MSBuildProjectExtension)' == '.tmp_proj' AND Exists('$(MSBuildProjectDirectory)\$(AssemblyName).vbproj'))">
			<PropertyGroup>
				<_SdkLanguageName>VisualBasic</_SdkLanguageName>
				<_SdkLanguageExtension>.vb</_SdkLanguageExtension>
				<_SdkLanguageGuid>F184B08F-C81C-45F6-A57F-5ABD9991F28F</_SdkLanguageGuid>
				<_SdkDesignerFolder>My Project</_SdkDesignerFolder>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.fsproj' OR ('$(MSBuildProjectExtension)' == '.tmp_proj' AND Exists('$(MSBuildProjectDirectory)\$(AssemblyName).fsproj'))">
			<PropertyGroup>
				<_SdkLanguageName>FSharp</_SdkLanguageName>
				<_SdkLanguageExtension>.fs</_SdkLanguageExtension>
				<_SdkLanguageGuid>F2A71F9B-5D33-465A-A702-920D77279786</_SdkLanguageGuid>
				<_SdkDesignerFolder>Properties</_SdkDesignerFolder>
			</PropertyGroup>
		</When>

		<Otherwise/>
	</Choose>

	<PropertyGroup>
		<DefaultLanguage Condition="'$(DefaultLanguage)' == ''">en</DefaultLanguage>
		<SupportedCultures Condition="'$(SupportedCultures)' == ''">en;ta;jp</SupportedCultures>
		<AppDesignerFolder Condition="'$(AppDesignerFolder)' == ''">$(_SdkDesignerFolder)</AppDesignerFolder>
		<_SdkVisualStudioVersion>$(VisualStudioVersion)</_SdkVisualStudioVersion>
		<_SdkVisualStudioVersion Condition="'$(_SdkVisualStudioVersion)' == '' OR '$(_SdkVisualStudioVersion)' &lt; '14.0'">15.0</_SdkVisualStudioVersion>
	</PropertyGroup>

	<Import Project="$(MSBuildThisFileDirectory)MSBuild.NET.ProjectSystem.props"/>
	<Import Project="$(MSBuildThisFileDirectory)MSBuild.NET.Extras.Items.props"/>
	<Import Project="$(MSBuildThisFileDirectory)Platforms\*.props"/>

</Project>