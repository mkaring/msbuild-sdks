<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<Import Project="$(MSBuildThisFileDirectory)MSBuild.NET.Workarounds.props"/>

	<Choose>
		<When Condition="'$(MSBuildProjectExtension)' == '.csproj'">
			<PropertyGroup>
				<_SdkLanguageName>CSharp</_SdkLanguageName>
				<_SdkLanguageExtension>.cs</_SdkLanguageExtension>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.vbproj'">
			<PropertyGroup>
				<_SdkLanguageName>VisualBasic</_SdkLanguageName>
				<_SdkLanguageExtension>.vb</_SdkLanguageExtension>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.fsproj'">
			<PropertyGroup>
				<_SdkLanguageName>FSharp</_SdkLanguageName>
				<_SdkLanguageExtension>.fs</_SdkLanguageExtension>
			</PropertyGroup>
		</When>

		<Otherwise/>
	</Choose>

	<PropertyGroup>
		<DefaultLanguage Condition="'$(DefaultLanguage)' == ''">$(LangName)</DefaultLanguage>
		<DefaultLanguage Condition="'$(DefaultLanguage)' == ''">$([System.Globalization.CultureInfo]::CurrentUICulture.Name)</DefaultLanguage>

		<SupportedCultures Condition="'$(SupportedCultures)' == ''">$(DefaultLanguage)</SupportedCultures>

		<_SdkVisualStudioVersion>$(VisualStudioVersion)</_SdkVisualStudioVersion>
		<_SdkVisualStudioVersion Condition="'$(_SdkVisualStudioVersion)' == '' OR '$(_SdkVisualStudioVersion)' &lt; '14.0'">15.0</_SdkVisualStudioVersion>
	</PropertyGroup>

	<PropertyGroup Condition="'$(AppDesignerFolder)' == ''">
		<AppDesignerFolder Condition="'$(_SdkLanguageName)' != 'VisualBasic'">Properties</AppDesignerFolder>
		<AppDesignerFolder Condition="'$(_SdkLanguageName)' == 'VisualBasic'">My Project</AppDesignerFolder>
	</PropertyGroup>

	<Import Project="$(MSBuildThisFileDirectory)MSBuild.NET.ProjectSystem.props"/>
	<Import Project="$(MSBuildThisFileDirectory)MSBuild.NET.Extras.Items.props"/>
	<Import Project="$(MSBuildThisFileDirectory)Platforms\*.props"/>

</Project>