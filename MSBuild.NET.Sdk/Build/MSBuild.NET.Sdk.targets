<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup Label="Globals">
		<IsMultiTargetingBuild Condition="'$(TargetFrameworks)' != '' AND '$(TargetFramework)' == ''">true</IsMultiTargetingBuild>
		<!-- TODO: Remove once the migration is done. Microsoft.CSharp.targets and Microsoft.VisualBasic.targets use 'IsCrossTargetingBuild'. -->
		<IsCrossTargetingBuild>$(IsMultiTargetingBuild)</IsCrossTargetingBuild>
	</PropertyGroup>

	<PropertyGroup>
		<CoreLanguageTargets>$(MSBuildThisFileDirectory)MSBuild.NET.Language.targets</CoreLanguageTargets>
	</PropertyGroup>

	<Import Project="$(MSBuildThisFileDirectory)MSBuild.NET.Core.targets" />

	<PropertyGroup>
		<ImportedMSBuildNETSdkTargets>true</ImportedMSBuildNETSdkTargets>
	</PropertyGroup>

</Project>