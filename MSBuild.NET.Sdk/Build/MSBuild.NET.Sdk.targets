<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup Label="Globals">
		<IsMultiTargetingBuild Condition="'$(TargetFrameworks)' != '' AND '$(TargetFramework)' == ''">true</IsMultiTargetingBuild>
		<!-- TODO: Remove once the migration is done. Microsoft.CSharp.targets and Microsoft.VisualBasic.targets use 'IsCrossTargetingBuild'. -->
		<IsCrossTargetingBuild>$(IsMultiTargetingBuild)</IsCrossTargetingBuild>
	</PropertyGroup>

	<PropertyGroup Label="Configure">
		<!-- Override CoreExtensions provided within the MSBuild.Core.Sdk -->
		<MSBuildCoreExtensionTargetsPath>$(MSBuildThisFileDirectory)MSBuild.NET.CoreExtension.targets</MSBuildCoreExtensionTargetsPath>
	</PropertyGroup>

	<!--<Import Project="Sdk.targets" Sdk="MSBuild.Core.Sdk" />-->
	<!--<Import Project="$(MSBuildCoreSdkPath)Sdks\Sdk.targets" />-->
	<Import Project="$(MSBuildCoreSdkPath)Build\MSBuild.Core.Sdk.targets" />

	<!--<Import Project="$(MicrosoftNETSdkBuildPath)Microsoft.NET.Sdk.targets"/>-->

	<PropertyGroup>
		<ImportedMSBuildNETSdkTargets>true</ImportedMSBuildNETSdkTargets>
	</PropertyGroup>

</Project>