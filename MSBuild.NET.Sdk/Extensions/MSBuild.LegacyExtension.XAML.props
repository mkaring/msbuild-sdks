<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<ImportByWildcardBeforeMicrosoftXAMLProps Condition="'$(ImportByWildcardBeforeMicrosoftXAMLProps)' == ''">true</ImportByWildcardBeforeMicrosoftXAMLProps>
		<ImportByWildcardAfterMicrosoftXAMLProps Condition="'$(ImportByWildcardAfterMicrosoftXAMLProps)' == ''">true</ImportByWildcardAfterMicrosoftXAMLProps>
		<ImportUserLocationsByWildcardBeforeMicrosoftXAMLProps Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftXAMLProps)' == ''">true</ImportUserLocationsByWildcardBeforeMicrosoftXAMLProps>
		<ImportUserLocationsByWildcardAfterMicrosoftXAMLProps Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftXAMLProps)' == ''">true</ImportUserLocationsByWildcardAfterMicrosoftXAMLProps>

		<_ImportBeforeMicrosoftXAMLPropsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.XAML.props\ImportBefore</_ImportBeforeMicrosoftXAMLPropsPath>
		<_ImportAfterMicrosoftXAMLPropsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.XAML.props\ImportAfter</_ImportAfterMicrosoftXAMLPropsPath>
		<_ImportUserLocationsBeforeMicrosoftXAMLPropsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.XAML.props\ImportBefore</_ImportUserLocationsBeforeMicrosoftXAMLPropsPath>
		<_ImportUserLocationsAfterMicrosoftXAMLPropsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.XAML.props\ImportAfter</_ImportUserLocationsAfterMicrosoftXAMLPropsPath>

		<CustomBeforeMicrosoftXAMLProps Condition="'$(CustomBeforeMicrosoftXAMLProps)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.Before.Microsoft.XAML.props</CustomBeforeMicrosoftXAMLProps>
		<CustomAfterMicrosoftXAMLProps Condition="'$(CustomAfterMicrosoftXAMLProps)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.After.Microsoft.XAML.props</CustomAfterMicrosoftXAMLProps>
	</PropertyGroup>

	<ImportGroup>
		<Import Project="$(_ImportUserLocationsBeforeMicrosoftXAMLPropsPath)\*" Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftXAMLProps)' == 'true' and Exists('$(_ImportUserLocationsBeforeMicrosoftXAMLPropsPath)')"/>
		<Import Project="$(_ImportBeforeMicrosoftXAMLPropsPath)\*" Condition="'$(ImportByWildcardBeforeMicrosoftXAMLProps)' == 'true' and Exists('$(_ImportBeforeMicrosoftXAMLPropsPath)')"/>
		<Import Project="$(CustomBeforeMicrosoftXAMLProps)" Condition="Exists('$(CustomBeforeMicrosoftXAMLProps)')" />

		<Import Project="$(MSBuildThisFileDirectory)..\Languages\MSBuild.NET.XAML.props" />

		<Import Project="$(CustomAfterMicrosoftXAMLProps)" Condition="Exists('$(CustomAfterMicrosoftXAMLProps)')" />
		<Import Project="$(_ImportUserLocationsAfterMicrosoftXAMLPropsPath)\*" Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftXAMLProps)' == 'true' and Exists('$(_ImportUserLocationsAfterMicrosoftXAMLPropsPath)')"/>
		<Import Project="$(_ImportAfterMicrosoftXAMLPropsPath)\*" Condition="'$(ImportByWildcardAfterMicrosoftXAMLProps)' == 'true' and Exists('$(_ImportAfterMicrosoftXAMLPropsPath)')"/>
	</ImportGroup>

</Project>