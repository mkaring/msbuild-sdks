<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<ImportByWildcardBeforeMicrosoftFSharpProps Condition="'$(ImportByWildcardBeforeMicrosoftFSharpProps)' == ''">true</ImportByWildcardBeforeMicrosoftFSharpProps>
		<ImportByWildcardAfterMicrosoftFSharpProps Condition="'$(ImportByWildcardAfterMicrosoftFSharpProps)' == ''">true</ImportByWildcardAfterMicrosoftFSharpProps>
		<ImportUserLocationsByWildcardBeforeMicrosoftFSharpProps Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftFSharpProps)' == ''">true</ImportUserLocationsByWildcardBeforeMicrosoftFSharpProps>
		<ImportUserLocationsByWildcardAfterMicrosoftFSharpProps Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftFSharpProps)' == ''">true</ImportUserLocationsByWildcardAfterMicrosoftFSharpProps>

		<_ImportBeforeMicrosoftFSharpPropsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.FSharp.props\ImportBefore</_ImportBeforeMicrosoftFSharpPropsPath>
		<_ImportAfterMicrosoftFSharpPropsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.FSharp.props\ImportAfter</_ImportAfterMicrosoftFSharpPropsPath>
		<_ImportUserLocationsBeforeMicrosoftFSharpPropsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.FSharp.props\ImportBefore</_ImportUserLocationsBeforeMicrosoftFSharpPropsPath>
		<_ImportUserLocationsAfterMicrosoftFSharpPropsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.FSharp.props\ImportAfter</_ImportUserLocationsAfterMicrosoftFSharpPropsPath>

		<CustomBeforeMicrosoftFSharpProps Condition="'$(CustomBeforeMicrosoftFSharpProps)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.Before.Microsoft.FSharp.props</CustomBeforeMicrosoftFSharpProps>
		<CustomAfterMicrosoftFSharpProps Condition="'$(CustomAfterMicrosoftFSharpProps)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.After.Microsoft.FSharp.props</CustomAfterMicrosoftFSharpProps>
	</PropertyGroup>

	<ImportGroup>
		<Import Project="$(_ImportUserLocationsBeforeMicrosoftFSharpPropsPath)\*" Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftFSharpProps)' == 'true' and Exists('$(_ImportUserLocationsBeforeMicrosoftFSharpPropsPath)')"/>
		<Import Project="$(_ImportBeforeMicrosoftFSharpPropsPath)\*" Condition="'$(ImportByWildcardBeforeMicrosoftFSharpProps)' == 'true' and Exists('$(_ImportBeforeMicrosoftFSharpPropsPath)')"/>
		<Import Project="$(CustomBeforeMicrosoftFSharpProps)" Condition="Exists('$(CustomBeforeMicrosoftFSharpProps)')" />

		<Import Project="$(MSBuildThisFileDirectory)..\Languages\MSBuild.NET.FSharp.props" />

		<Import Project="$(CustomAfterMicrosoftFSharpProps)" Condition="Exists('$(CustomAfterMicrosoftFSharpProps)')" />
		<Import Project="$(_ImportUserLocationsAfterMicrosoftFSharpPropsPath)\*" Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftFSharpProps)' == 'true' and Exists('$(_ImportUserLocationsAfterMicrosoftFSharpPropsPath)')"/>
		<Import Project="$(_ImportAfterMicrosoftFSharpPropsPath)\*" Condition="'$(ImportByWildcardAfterMicrosoftFSharpProps)' == 'true' and Exists('$(_ImportAfterMicrosoftFSharpPropsPath)')"/>
	</ImportGroup>

</Project>