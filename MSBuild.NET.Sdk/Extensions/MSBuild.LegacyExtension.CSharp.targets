<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<ImportByWildcardBeforeMicrosoftCSharpTargets Condition="'$(ImportByWildcardBeforeMicrosoftCSharpTargets)' == ''">true</ImportByWildcardBeforeMicrosoftCSharpTargets>
		<ImportByWildcardAfterMicrosoftCSharpTargets Condition="'$(ImportByWildcardAfterMicrosoftCSharpTargets)' == ''">true</ImportByWildcardAfterMicrosoftCSharpTargets>
		<ImportUserLocationsByWildcardBeforeMicrosoftCSharpTargets Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftCSharpTargets)' == ''">true</ImportUserLocationsByWildcardBeforeMicrosoftCSharpTargets>
		<ImportUserLocationsByWildcardAfterMicrosoftCSharpTargets Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftCSharpTargets)' == ''">true</ImportUserLocationsByWildcardAfterMicrosoftCSharpTargets>

		<_ImportBeforeMicrosoftCSharpTargetsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.targets\ImportBefore</_ImportBeforeMicrosoftCSharpTargetsPath>
		<_ImportAfterMicrosoftCSharpTargetsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.targets\ImportAfter</_ImportAfterMicrosoftCSharpTargetsPath>
		<_ImportUserLocationsBeforeMicrosoftCSharpTargetsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.targets\ImportBefore</_ImportUserLocationsBeforeMicrosoftCSharpTargetsPath>
		<_ImportUserLocationsAfterMicrosoftCSharpTargetsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.targets\ImportAfter</_ImportUserLocationsAfterMicrosoftCSharpTargetsPath>

		<CustomBeforeMicrosoftCSharpTargets Condition="'$(CustomBeforeMicrosoftCSharpTargets)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.Before.Microsoft.CSharp.targets</CustomBeforeMicrosoftCSharpTargets>
		<CustomAfterMicrosoftCSharpTargets Condition="'$(CustomAfterMicrosoftCSharpTargets)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.After.Microsoft.CSharp.targets</CustomAfterMicrosoftCSharpTargets>
	</PropertyGroup>

	<ImportGroup>
		<Import Project="$(_ImportUserLocationsBeforeMicrosoftCSharpTargetsPath)\*" Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftCSharpTargets)' == 'true' and Exists('$(_ImportUserLocationsBeforeMicrosoftCSharpTargetsPath)')"/>
		<Import Project="$(_ImportBeforeMicrosoftCSharpTargetsPath)\*" Condition="'$(ImportByWildcardBeforeMicrosoftCSharpTargets)' == 'true' and Exists('$(_ImportBeforeMicrosoftCSharpTargetsPath)')"/>
		<Import Project="$(CustomBeforeMicrosoftCSharpTargets)" Condition="Exists('$(CustomBeforeMicrosoftCSharpTargets)')" />

		<Import Project="$(MSBuildThisFileDirectory)..\Languages\MSBuild.NET.CSharp.targets" />

		<Import Project="$(CustomAfterMicrosoftCSharpTargets)" Condition="Exists('$(CustomAfterMicrosoftCSharpTargets)')" />
		<Import Project="$(_ImportUserLocationsAfterMicrosoftCSharpTargetsPath)\*" Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftCSharpTargets)' == 'true' and Exists('$(_ImportUserLocationsAfterMicrosoftCSharpTargetsPath)')"/>
		<Import Project="$(_ImportAfterMicrosoftCSharpTargetsPath)\*" Condition="'$(ImportByWildcardAfterMicrosoftCSharpTargets)' == 'true' and Exists('$(_ImportAfterMicrosoftCSharpTargetsPath)')"/>
	</ImportGroup>

</Project>