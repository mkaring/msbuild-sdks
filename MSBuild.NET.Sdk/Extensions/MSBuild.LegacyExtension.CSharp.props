<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<ImportByWildcardBeforeMicrosoftCSharpProps Condition="'$(ImportByWildcardBeforeMicrosoftCSharpProps)' == ''">true</ImportByWildcardBeforeMicrosoftCSharpProps>
		<ImportByWildcardAfterMicrosoftCSharpProps Condition="'$(ImportByWildcardAfterMicrosoftCSharpProps)' == ''">true</ImportByWildcardAfterMicrosoftCSharpProps>
		<ImportUserLocationsByWildcardBeforeMicrosoftCSharpProps Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftCSharpProps)' == ''">true</ImportUserLocationsByWildcardBeforeMicrosoftCSharpProps>
		<ImportUserLocationsByWildcardAfterMicrosoftCSharpProps Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftCSharpProps)' == ''">true</ImportUserLocationsByWildcardAfterMicrosoftCSharpProps>

		<_ImportBeforeMicrosoftCSharpPropsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.props\ImportBefore</_ImportBeforeMicrosoftCSharpPropsPath>
		<_ImportAfterMicrosoftCSharpPropsPath>$(MSBuildExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.props\ImportAfter</_ImportAfterMicrosoftCSharpPropsPath>
		<_ImportUserLocationsBeforeMicrosoftCSharpPropsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.props\ImportBefore</_ImportUserLocationsBeforeMicrosoftCSharpPropsPath>
		<_ImportUserLocationsAfterMicrosoftCSharpPropsPath>$(MSBuildUserExtensionsPath)\$(_MSBuildToolsVersion)\Microsoft.CSharp.props\ImportAfter</_ImportUserLocationsAfterMicrosoftCSharpPropsPath>

		<CustomBeforeMicrosoftCSharpProps Condition="'$(CustomBeforeMicrosoftCSharpProps)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.Before.Microsoft.CSharp.props</CustomBeforeMicrosoftCSharpProps>
		<CustomAfterMicrosoftCSharpProps Condition="'$(CustomAfterMicrosoftCSharpProps)' == ''">$(MSBuildExtensionsPath)\v$(_MSBuildToolsVersion)\Custom.After.Microsoft.CSharp.props</CustomAfterMicrosoftCSharpProps>
	</PropertyGroup>

	<ImportGroup>
		<Import Project="$(_ImportUserLocationsBeforeMicrosoftCSharpPropsPath)\*" Condition="'$(ImportUserLocationsByWildcardBeforeMicrosoftCSharpProps)' == 'true' and Exists('$(_ImportUserLocationsBeforeMicrosoftCSharpPropsPath)')"/>
		<Import Project="$(_ImportBeforeMicrosoftCSharpPropsPath)\*" Condition="'$(ImportByWildcardBeforeMicrosoftCSharpProps)' == 'true' and Exists('$(_ImportBeforeMicrosoftCSharpPropsPath)')"/>
		<Import Project="$(CustomBeforeMicrosoftCSharpProps)" Condition="Exists('$(CustomBeforeMicrosoftCSharpProps)')" />

		<Import Project="$(MSBuildThisFileDirectory)..\Languages\MSBuild.NET.CSharp.props" />

		<Import Project="$(CustomAfterMicrosoftCSharpProps)" Condition="Exists('$(CustomAfterMicrosoftCSharpProps)')" />
		<Import Project="$(_ImportUserLocationsAfterMicrosoftCSharpPropsPath)\*" Condition="'$(ImportUserLocationsByWildcardAfterMicrosoftCSharpProps)' == 'true' and Exists('$(_ImportUserLocationsAfterMicrosoftCSharpPropsPath)')"/>
		<Import Project="$(_ImportAfterMicrosoftCSharpPropsPath)\*" Condition="'$(ImportByWildcardAfterMicrosoftCSharpProps)' == 'true' and Exists('$(_ImportAfterMicrosoftCSharpPropsPath)')"/>
	</ImportGroup>

</Project>