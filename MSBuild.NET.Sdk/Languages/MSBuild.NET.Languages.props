<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<Choose>
		<When Condition="'$(MSBuildProjectExtension)' == '.csproj'">
			<PropertyGroup>
				<_SdkLanguageName>CSharp</_SdkLanguageName>
				<_SdkLanguageSymbol>C#</_SdkLanguageSymbol>
				<_SdkLanguageExtension>.cs</_SdkLanguageExtension>
			</PropertyGroup>
		</When>
	
		<When Condition="'$(MSBuildProjectExtension)' == '.fsproj'">
			<PropertyGroup>
				<_SdkLanguageName>FSharp</_SdkLanguageName>
				<_SdkLanguageSymbol>F#</_SdkLanguageSymbol>
				<_SdkLanguageExtension>.fs</_SdkLanguageExtension>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.vbproj'">
			<PropertyGroup>
				<_SdkLanguageName>VisualBasic</_SdkLanguageName>
				<_SdkLanguageSymbol>VB</_SdkLanguageSymbol>
				<_SdkLanguageExtension>.vb</_SdkLanguageExtension>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.cilproj'">
			<PropertyGroup>
				<_SdkLanguageName>CommonIntermediateLanguage</_SdkLanguageName>
				<_SdkLanguageSymbol>CIL</_SdkLanguageSymbol>
				<_SdkLanguageExtension>.cil</_SdkLanguageExtension>
			</PropertyGroup>
		</When>

		<Otherwise>
			<PropertyGroup>
				<_SdkLanguageName>Common</_SdkLanguageName>
			</PropertyGroup>
		</Otherwise>
	</Choose>

	<PropertyGroup>
		<LanguageTargets>$(MSBuildThisFileDirectory)MSBuild.NET.Language.$(_SdkLanguageName).targets</LanguageTargets>
	</PropertyGroup>

</Project>