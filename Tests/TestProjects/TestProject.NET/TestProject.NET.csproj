<Project Sdk="MSBuild.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard1.0;netstandard2.0</TargetFrameworks>
		<TargetFrameworks>$(TargetFrameworks);netcoreapp1.0;netcoreapp2.1</TargetFrameworks>
		<TargetFrameworks>$(TargetFrameworks);net40;net45;net47</TargetFrameworks>
		<!--<TargetFrameworks>$(TargetFrameworks);uap10.0;uap10.0.14393;uap10.0.16299</TargetFrameworks>-->
		<!--<TargetFrameworks>$(TargetFrameworks);monoandroid70;xamarin.ios10;xamarin.mac20</TargetFrameworks>-->
		<!--<TargetFrameworks>$(TargetFrameworks);tizen30;tizen40</TargetFrameworks>-->
		<!--<TargetFrameworks>$(TargetFrameworks);net20;net35</TargetFrameworks>-->
		<IsPackable>false</IsPackable>
	</PropertyGroup>

	<PropertyGroup Condition="$(TargetFramework) == 'netcoreapp2.1'">
		<ImplicitFrameworkPackageVersion>2.1.0-*</ImplicitFrameworkPackageVersion>
	</PropertyGroup>

	<ItemGroup Condition="$(TargetFramework) == 'netcoreapp2.1'">
		<PackageReference Include="Microsoft.AspNetCore.App" Version="$(ImplicitFrameworkPackageVersion)" IsImplicitlyDefined="true"/>
	</ItemGroup>

	<!--<ItemGroup>
		<PackageReference Include="MSBuild.NET.Extras.Sdk" Version="1.2.0" PrivateAssets="All"/>
	</ItemGroup>-->

	<!--<ItemGroup Condition="'$(TargetFramework)' == 'tizen40'">
		<PackageReference Include="Tizen.NET" Version="4.0.0"/>
	</ItemGroup>-->

</Project>